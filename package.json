{
  "name": "beater-tap-reporter",
  "description": "beater TAP reporter",
  "version": "1.0.3",
  "author": {
    "name": "bouzuya",
    "email": "m@bouzuya.net",
    "url": "http://bouzuya.net/"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": 8
          }
        }
      ],
      "power-assert"
    ]
  },
  "bugs": {
    "url": "https://github.com/bouzuya/beater-tap-reporter/issues"
  },
  "dependencies": {
    "beater-reporter": "^0.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@types/node": "^12.0.0",
    "@types/power-assert": "^1.5.0",
    "@types/proxyquire": "^1.3.28",
    "@types/sinon": "^7.0.11",
    "babel-preset-power-assert": "^3.0.0",
    "beater": "^6.0.1",
    "cpx": "^1.5.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "power-assert": "^1.6.1",
    "proxyquire": "^2.1.0",
    "rimraf": "^2.6.3",
    "sinon": "^7.3.2",
    "typescript": "^3.4.5",
    "watch": "^1.0.2"
  },
  "files": [
    "lib"
  ],
  "homepage": "https://github.com/bouzuya/beater-tap-reporter",
  "keywords": [
    "beater",
    "beater-reporter",
    "reporter",
    "tap"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/bouzuya/beater-tap-reporter.git"
  },
  "scripts": {
    "build": "npm-run-all -s \"build:tmp:ts\" \"build:tmp:es2017\" \"build:lib\"",
    "build:lib": "npm-run-all -p \"build:lib:*\"",
    "build:lib:dts": "cpx \".tmp/es2017/src/**/*.d.ts\" \"lib\"",
    "build:lib:js": "cpx \".tmp/es5/src/**/*.js\" \"lib\"",
    "build:tmp:es2017": "babel .tmp/es2017/ -d .tmp/es5/",
    "build:tmp:ts": "tsc",
    "clean": "rimraf \".tmp\" \"lib\"",
    "prepublish": "npm-run-all -s \"clean\" \"build\"",
    "test": "nyc node \".tmp/es5/test/index.js\"",
    "watch": "npm-run-all -p \"watch:*\"",
    "watch:es2017": "npm run \"build:tmp:es2017\" -- --watch",
    "watch:lib": "watch --wait 2 \"npm run build:lib\" \".tmp/es5/src/\"",
    "watch:test": "watch --wait 2 \"npm test\" \".tmp/es5/src\" \".tmp/es5/test\"",
    "watch:ts": "npm run \"build:tmp:ts\" -- --watch"
  },
  "types": "lib/index.d.ts"
}
